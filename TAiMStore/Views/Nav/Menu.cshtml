@model Dictionary<TAiMStore.Model.ViewModels.CategoryViewModel, double>

@Html.ActionLink("Домой", "List", "Product", null,
    new { @class = "btn btn-block btn-default btn-lg" })

@foreach (var link in Model)
{
    
    @Html.RouteLink(link.Key.CategoryName, new
    {
        controller = "Product",
        action = "List",
        category = link.Key.CategoryName,
        page = 1
    }, new
    {
        @class ="btn btn-block btn-default btn-lg"
        + (link.Key.CategoryName == ViewBag.SelectedCategory ? " btn-primary" : "")
     })
}